    "kubernetes_cron_job": {
      "dns_route53_update_60C50A01": {
        "//": {
          "metadata": {
            "path": "dns/route-53/update",
            "uniqueId": "dns_route53_update_60C50A01"
          }
        },
        "metadata": {
          "generate_name": "update-route-53-",
          "namespace": "${kubernetes_namespace.dns_9A634489.metadata[0].name}"
        },
        "spec": {
          "concurrency_policy": "Forbid",
          "failed_jobs_history_limit": 1,
          "job_template": {
            "metadata": {
              "generate_name": "update-route-53-"
            },
            "spec": {
              "backoff_limit": 0,
              "template": {
                "metadata": {
                  "generate_name": "update-route-53-"
                },
                "spec": {
                  "container": [
                    {
                      "command": [
                        "/bin/bash",
                        "-c",
                        "${file(\"../../../src/dns/route53-ddns.sh\")}"
                      ],
                      "env": [
                        {
                          "name": "AWS_ACCESS_KEY_ID",
                          "value": "${aws_iam_access_key.dns_route53_ddnsaccesskey_AFB46BAD.id}"
                        },
                        {
                          "name": "AWS_SECRET_ACCESS_KEY",
                          "value": "${aws_iam_access_key.dns_route53_ddnsaccesskey_AFB46BAD.secret}"
                        },
                        {
                          "name": "AWS_DEFAULT_REGION",
                          "value": "us-west-2"
                        },
                        {
                          "name": "NAME",
                          "value": "nregner.net."
                        },
                        {
                          "name": "HOSTED_ZONE_ID",
                          "value": "${aws_route53_zone.dns_zone_2E2729FB.id}"
                        }
                      ],
                      "image": "amazon/aws-cli:2.4.27",
                      "name": "route-53-0"
                    }
                  ],
                  "restart_policy": "Never"
                }
              }
            }
          },
          "schedule": "*/15 * * * *",
          "successful_jobs_history_limit": 1
        }
      }
