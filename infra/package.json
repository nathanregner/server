{
  "name": "infra",
  "version": "1.0.0",
  "private": true,
  "main": "dist/main.js",
  "source": "main.ts",
  "scripts": {
    "build": "parcel build",
    "check:watch": "tsc -w --noEmit",
    "format": "prettier --write **/*.{ts,json}",
    "generate:cdktf": "cdktf get",
    "generate:manifests": "npm run compile && node target/src/manifest-generator.js",
    "synth:watch": "mkdir dist; touch dist/main.js; parcel watch & nodemon --watch dist/main.js --exec node dist/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "npm-check-updates -u"
  },
  "dependencies": {
    "@cdktf/provider-helm": "^0.3.52",
    "@cdktf/provider-kubernetes": "^0.6.53",
    "@kubernetes/client-node": "^0.16.3",
    "@swc/helpers": "^0.3.8",
    "aws-sdk": "^2.1100.0",
    "cdktf": "^0.9.4",
    "constructs": "^10.0.94",
    "json-schema-to-typescript": "^10.1.5"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.4.0",
    "@parcel/transformer-typescript-types": "^2.4.0",
    "@parcel/validator-typescript": "^2.4.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "cdktf-cli": "^0.9.4",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "npm-check-updates": "^12.5.4",
    "parcel": "^2.4.0",
    "prettier": "^2.6.0",
    "ts-jest": "^27.1.4",
    "tsc-watch": "^4.6.2",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=16"
  },
  "targets": {
    "main": {
      "source": "main.ts",
      "context": "node"
    },
    "generate-manifests": {
      "source": "generate-manifests.ts",
      "context": "node"
    }
  }
}
