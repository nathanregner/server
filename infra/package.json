{
  "name": "infra",
  "version": "1.0.0",
  "private": true,
  "license": "MPL-2.0",
  "main": "main.js",
  "types": "main.ts",
  "scripts": {
    "build": "tsc",
    "compile": "tsc --pretty",
    "deploy:watch": "cdktf watch --auto-approve",
    "format": "prettier --write **/*.{ts,json}",
    "get": "cdktf get",
    "synth": "cdktf synth",
    "synth:watch": "tsc-watch --onSuccess \"node target/main.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@cdktf/provider-google": "^0.5.129",
    "@cdktf/provider-helm": "^0.2.144",
    "@cdktf/provider-kubernetes": "^0.5.129",
    "cdktf": "^0.8.5",
    "constructs": "^10.0.27"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=10.12"
  }
}
