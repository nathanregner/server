{
  "name": "infra",
  "version": "1.0.0",
  "private": true,
  "license": "MPL-2.0",
  "main": "main.js",
  "types": "main.ts",
  "scripts": {
    "build": "tsc",
    "compile": "tsc --pretty",
    "deploy:watch": "cdktf watch --auto-approve",
    "format": "prettier --write **/*.{ts,json}",
    "get": "cdktf get",
    "manifests:generate": "npm run compile && node target/src/manifest-generator.js",
    "synth": "cdktf synth",
    "synth:watch": "tsc-watch --onSuccess \"node target/main.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "npm-check-updates -u",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@cdktf/provider-aws": "^5.0.52",
    "@cdktf/provider-google": "^0.6.53",
    "@cdktf/provider-helm": "^0.3.52",
    "@cdktf/provider-kubernetes": "^0.6.53",
    "cdktf": "^0.9.4",
    "constructs": "^10.0.92"
  },
  "devDependencies": {
    "@kubernetes/client-node": "^0.16.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.22",
    "jest": "^27.5.1",
    "json-schema-to-typescript": "^10.1.5",
    "npm-check-updates": "^12.5.4",
    "prettier": "^2.6.0",
    "ts-jest": "^27.1.3",
    "tsc-watch": "^4.6.2",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">=10.12"
  }
}
